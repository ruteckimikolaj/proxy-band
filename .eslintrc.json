{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["import", "prettier"],
    "rules": {
      "semi": 0,
      "comma-dangle": 0,
      "prettier/prettier": "error"
    }
}